# Frontend Production Environment
# Marketing Agent v2.0

# Environment identifier (development | staging | production)
VITE_ENV=production

# API Configuration - Update these for production
VITE_API_URL=https://api.yourdomain.com
VITE_API_WS_URL=wss://api.yourdomain.com

# App Metadata
VITE_APP_NAME=Marketing Agent
VITE_APP_VERSION=2.0.0
VITE_APP_BUILD_ENV=production

# Feature Flags (production)
VITE_ENABLE_DEBUG=false
VITE_ENABLE_MOCK_DATA=false
VITE_ENABLE_DEV_TOOLS=false

# Analytics (enabled in production)
VITE_ENABLE_ANALYTICS=true
